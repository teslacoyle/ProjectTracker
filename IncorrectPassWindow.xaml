<Window x:Name="IncorrectPassWindow1" x:Class="ProjectTracker.IncorrectPassWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectTracker"
        mc:Ignorable="d"
        Title="Incorrect Password" Height="119" Width="200">
    <StackPanel>
        <TextBlock TextWrapping="Wrap" Margin="0,10,-1.667,0" TextAlignment="Center" Text="The password you entered was incorrect."/>
        <Button Content="Close" Margin="62,10,62,0" Click="Button_Click"/>
    </StackPanel>
</Window>
