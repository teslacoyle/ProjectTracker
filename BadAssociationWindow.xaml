<Window x:Class="ProjectTracker.BadAssociationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectTracker"
        mc:Ignorable="d"
        Title="Wrong User" Height="127.333" Width="200">
    <StackPanel>
        <TextBlock TextWrapping="Wrap" Text="You are not associated with this project. please login as the correct user to edit or delete it." Margin="5,10,5,0" TextAlignment="Center"/>
        <Button Content="Close" Margin="62,10,62,0" Click="Button_Click"/>
    </StackPanel>
</Window>
